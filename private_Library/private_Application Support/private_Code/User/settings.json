	"latex-workshop.latex.recipes": [
	  {
		"name": "LuaLaTeX",
		"tools": ["lualatex"]
	  },
	  {
		"name": "LuaLaTeX Twice Compile",
		"tools": ["lualatex", "lualatex halt-on-error"]
	  },
	  {
		"name": "卒業研究報告書",
		"tools": ["platex", "pbibtex", "platex", "platex", "dvipdfmx"]
	  },
	  {
		"name": "LuaLaTex -> pdfcrop (no margins)",
		"tools": ["lualatex", "pdfcrop (no margins)"]
	  },
	  {
		"name": "LuaLaTex -> pdfcrop (with margins)",
		"tools": ["lualatex", "pdfcrop (with margins)"]
	  },
	  {
		"name": "latexindent",
		"tools": ["latexindent"]
	  },
	  {
		"name": "Convert to PNG",
		"tools": ["pdf2png"]
	  },
	  {
		"name": "Convert to SVG",
		"tools": ["pdf2svg"]
	  }
	],
	"latex-workshop.latex.tools": [
	  {
		"name": "lualatex",
		"command": "lualatex",
		"args": [
		  "-shell-escape",
		  "--cmdx",
		  "-file-line-error",
		  "-synctex=1",
		  "-interaction=nonstopmode",
		  "--output-directory=%OUTDIR%",
		  "%DOC%"
		]
	  },
	  {
		"name": "lualatex halt-on-error",
		"command": "lualatex",
		"args": [
		  "--cmdx",
		  "-halt-on-error",
		  "-synctex=1",
		  "--output-directory=%OUTDIR%",
		  "%DOC%"
		]
	  },
	  {
		"name": "latexmk-lualatex",
		"command": "latexmk",
		"args": [
		  "-norc",
		  "-cd",
		  "-lualatex",
		  "-synctex=1",
		  "-interaction=nonstopmode",
		  "-file-line-error",
		  "-outdir=%OUTDIR%",
		  "%DOC%"
		]
	  },
	  {
		"name": "platex",
		"command": "platex",
		"args": [
		  "-interaction=nonstopmode",
		  "-halt-on-error",
		  "-synctex=1",
		  "--output-directory=%OUTDIR%",
		  "%DOC%"
		]
	  },
	  {
		"name": "pbibtex",
		"command": "pbibtex",
		"args": ["%OUTDIR%\\%DOCFILE%"]
	  },
	  {
		"name": "dvipdfmx",
		"command": "dvipdfmx",
		"args": ["%OUTDIR%\\%DOCFILE%.dvi", "-o", "%OUTDIR%\\%DOCFILE%.pdf"]
	  },
	  {
		"name": "latexindent",
		"command": "latexindent",
		"args": [
		  "-c",
		  "%DIR%",
		  "%DOC_EXT%",
		  "-l",
		  "%DIR%/latexindent.yaml",
		  "-r",
		  "-w"
		]
	  },
	  {
		"name": "pdf2png",
		"command": "pdftocairo",
		"args": [
		  "-png",
		  "-r",
		  "300",
		  "%OUTDIR%\\%DOCFILE%.pdf",
		  "%OUTDIR%\\%DOCFILE%"
		]
	  },
	  {
		"name": "pdf2svg",
		"command": "pdftocairo",
		"args": ["-svg", "%OUTDIR%\\%DOCFILE%.pdf", "%OUTDIR%\\%DOCFILE%.svg"]
	  },
	  {
		"name": "pdfcrop (no margins)",
		"command": "pdfcrop",
		"args": ["%OUTDIR%\\%DOCFILE%.pdf", "%OUTDIR%\\%DOCFILE%.pdf"]
	  },
	  {
		"name": "pdfcrop (with margins)",
		"command": "pdfcrop",
		"args": [
		  "--margins",
		  "5",
		  "%OUTDIR%\\%DOCFILE%.pdf",
		  "%OUTDIR%\\%DOCFILE%.pdf"
		]
	  },
	  {
		"name": "latexmk-lualatex(legacy)",
		"command": "latexmk",
		"args": [
		  "-norc",
		  "-cd",
		  "-lualatex",
		  "-e",
		  "$lualatex=q/lualatex %O -synctex=1 -interaction=nonstopmode -file-line-error %S/",
		  "-e",
		  "$bibtex=q/upbibtex %O %B/",
		  "-e",
		  "$biber=q/biber %O --bblencoding=utf8 -u -U --output_safechars %B/",
		  "-e",
		  "$makeindex=q/upmendex %O -o %D %S/",
		  "-gg",
		  "-outdir=%OUTDIR%",
		  "%DOC%"
		]
	  }
	],
